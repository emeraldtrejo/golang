package main

import "fmt"

func main() {
  str1 := "The quick red fox"
  str2 := "jumped over"
  str3 := "the lazy brown dog."
  
  //anumber :=42
  //isTrue := true
  
  stringLength, err := fmt.Println(str1, str2, str3)
  
  #check if got a non nil error back
  if err == nil {
    fmt.Println("String length:", stringLength)
  }

}

#in CL
go run consoleOutput.go

fmt.Printf("Value of aNumber: %v\n", aNumber)
fmt.Printf("Value of isTrue: %v\n", isTrue)
#give floating number with 2 decimal points
fmt.Printf("Value of aNumbeer as float: %.2f\n", aNumber)

fmt.Printf("Data types: %T, %T, %T, %T and %T", str1, str2, str3,aNumber, isTrue)

myString := fmt.Sprintf("Data types as var: %T, %T, %T, %T and %T", str1, str2, str3,aNumber, isTrue)
fmt.Print(myString)

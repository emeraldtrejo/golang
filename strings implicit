package main

import (
  "fmt"
)

func main() {
  str1 := "An implicitly typed string"
  fmt.Printf("sr1: %v:%T\n str1, str1)
  
  fmt.Println(strings.ToUpper(str1))
  fmt.Println(strings.Title(str1))
  
  lValue := "hello"
  uValue :="HELLO"
  fmt.Println("Eqqual?", (lValue == uValue))
  fmt.Println("Eqqual non case senstive?", strings.EqualFold(lValue, uValue))
  
  fmt.Println("contains expression?", stings.Contains(str1, "exp"))
  fmt.Println("contains expression?", stings.Contains(str2, "exp"))
}
